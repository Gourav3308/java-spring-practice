package in.gm.main;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.Statement;

public class StVsPpSt {
	
	public static void main(String[] args) {
		
		try {
			Class.forName("com.mysql.cj.jdbc.Driver");
			Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/newjdbc","root", "Sonu@12345");
						
			Statement st=con.createStatement();
			//st.addBatch("insert into details values('Sonu','Araria',6)");
			//st.addBatch("insert into details values('Gourav','Purnia',9)");
			st.addBatch("update details set name='Mahi' where ward_no=9)");
			int [] count=st.executeBatch();
			
			for( int i : count) {
				System.out.println(i+ " success");
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

}
